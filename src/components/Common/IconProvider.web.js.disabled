// Base Icon Component
const Icon = ({ name, size = 24, color = "#000000", style, ...props }) => {
  const IconComponent = iconMap[name];

  if (IconComponent) {
    return (
      <IconComponent
        style={{ fontSize: size, color, ...style }}
        {...props}
      />
    );
  }

  // Fallback for unknown icons
  return <span style={{ fontSize: size, color, ...style }}>â—‹</span>;
};

