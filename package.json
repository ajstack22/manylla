{
  "name": "manylla",
  "version": "2025.09.14.2",
  "private": true,
  "homepage": "/qual",
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "echo '⚠️  This starts React Native Metro (mobile). For web dev, use: npm run web' && react-native start",
    "start:reset": "react-native start --reset-cache",
    "web": "echo '🌐 Starting WEB dev server on http://localhost:3000 (webpack)' && webpack serve --mode development",
    "build:web": "NODE_ENV=production webpack --mode production",
    "test": "jest",
    "test:ci": "jest --ci --coverage --watchAll=false --bail",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:dev": "API_ENV=dev jest --testNamePattern='Integration'",
    "test:dev:all": "API_ENV=dev jest",
    "test:qual": "API_ENV=qual jest --testNamePattern='Integration'",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json}\"",
    "typecheck": "tsc --noEmit",
    "check:type": "node scripts/check-project-type.js",
    "license:check": "npx license-checker --summary --failOn 'GPL;AGPL;LGPL'",
    "license:report": "npx license-checker --out LICENSES-REPORT.txt",
    "license:report:json": "npx license-checker --json --out licenses-report.json",
    "clean": "cd android && ./gradlew clean && cd ../ios && rm -rf build && cd ..",
    "pod-install": "cd ios && pod install && cd ..",
    "_DEPRECATED_deploy:qual": "echo '❌ DO NOT USE - Run ./scripts/deploy-qual.sh instead'"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^6.5.0",
    "@mui/material": "^6.5.0",
    "@react-native-async-storage/async-storage": "^2.1.0",
    "@react-native-community/datetimepicker": "^8.4.4",
    "@react-native-picker/picker": "^2.11.2",
    "@react-navigation/bottom-tabs": "^7.0.0",
    "@react-navigation/native": "^7.0.0",
    "@react-navigation/native-stack": "^7.1.0",
    "@react-navigation/stack": "^7.0.0",
    "pako": "^2.1.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.1",
    "react-native-draggable-flatlist": "^4.0.3",
    "react-native-gesture-handler": "^2.28.0",
    "react-native-get-random-values": "^1.11.0",
    "react-native-image-picker": "^7.2.3",
    "react-native-qrcode-svg": "^6.3.15",
    "react-native-reanimated": "^3.16.5",
    "react-native-safe-area-context": "^5.6.1",
    "react-native-screens": "^4.4.0",
    "react-native-svg": "^15.12.1",
    "react-native-vector-icons": "^10.2.0",
    "react-native-web": "^0.21.1",
    "react-native-webview": "^13.16.0",
    "tweetnacl": "^1.0.3",
    "tweetnacl-util": "^0.15.1",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-react": "^7.27.1",
    "@babel/runtime": "^7.24.5",
    "@react-native-community/cli": "^15.0.1",
    "@react-native/babel-preset": "0.81.1",
    "@react-native/eslint-config": "0.81.1",
    "@react-native/metro-config": "0.81.1",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@testing-library/react-native": "^13.3.3",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "babel-jest": "^29.7.0",
    "babel-loader": "^10.0.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "babel-plugin-react-native-web": "^0.21.1",
    "copy-webpack-plugin": "^13.0.1",
    "css-loader": "^7.1.2",
    "eslint": "^8.57.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-import-resolver-babel-module": "^5.3.2",
    "eslint-plugin-react": "^7.37.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "html-webpack-plugin": "^5.6.4",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.1.2",
    "license-checker": "^25.0.1",
    "msw": "^2.11.2",
    "prettier": "^3.4.1",
    "react-test-renderer": "^19.1.0",
    "style-loader": "^4.0.0",
    "terser-webpack-plugin": "^5.3.14",
    "ts-loader": "^9.5.4",
    "typescript": "^5.9.2",
    "webpack": "^5.101.3",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2",
    "workbox-webpack-plugin": "^7.3.0"
  },
  "engines": {
    "node": ">=18"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.js"
    ],
    "testEnvironment": "jsdom",
    "testTimeout": 10000,
    "bail": true,
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(react-native|@react-native|react-native-vector-icons|msw)/)"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts",
      "!src/index.js",
      "!src/test/**",
      "!src/**/__tests__/**",
      "!src/polyfills/**",
      "!src/**/node_modules/**"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 7,
        "branches": 7,
        "functions": 7,
        "lines": 7
      },
      "src/services/sync/manyllaEncryptionService.js": {
        "statements": 60,
        "branches": 60,
        "functions": 60,
        "lines": 60
      },
      "src/services/sync/manyllaMinimalSyncServiceWeb.js": {
        "statements": 60,
        "branches": 60,
        "functions": 60,
        "lines": 60
      },
      "src/context/SyncContext.{js,tsx}": {
        "statements": 50,
        "branches": 47,
        "functions": 50,
        "lines": 50
      },
      "src/context/ThemeContext.{js,tsx}": {
        "statements": 50,
        "branches": 50,
        "functions": 50,
        "lines": 50
      }
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
